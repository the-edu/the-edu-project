{
  "name": "the-edu-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "tsc": "tsc --noEmit",
    "lint:fix": "eslint src --fix && npm run format",
    "lint:strict": "eslint --max-warnings=0 src",
    "format": "prettier --write */**/*.{js,jsx,json,ts,tsx,scss,css,md}",
    "format:check": "prettier -c */**/*.{js,jsx,json,ts,tsx,scss,css,md}",
    "check-types": "tsc --noemit"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@suspensive/react": "^2.18.12",
    "@tanstack/react-query": "^5.71.0",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "next": "15.2.4",
    "radix-ui": "^1.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.55.0",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^9.6.0",
    "@tailwindcss/postcss": "^4",
    "@tanstack/eslint-plugin-query": "^5.68.0",
